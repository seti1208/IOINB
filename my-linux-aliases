alias ls='ls --color=tty'
alias mc='. /usr/lib64/mc/mc-wrapper.sh'
alias pdb.py='python -m pdb'
alias pdb3.5.py='python3.5 -m pdb'
alias profile.py='python -m profile'
alias profile3.5.py='python3.5 -m profile'
alias pstats.py='python -m pstats'
alias pstats3.5.py='python3.5 -m pstats'
alias pygettext3.5.py='pygettext3.5'
alias smtpd.py='python -m smtpd'
alias smtpd3.5.py='python3.5 -m smtpd'
alias timeit.py='python -m timeit'
alias timeit3.5.py='python3.5 -m timeit'
alias webbrowser.py='python -m webbrowser'
alias webbrowser3.5.py='python3.5 -m webbrowser'
alias which='alias | /usr/bin/which --tty-only --read-alias --show-dot --show-tilde'
